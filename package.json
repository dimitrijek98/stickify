{
  "name": "Stickify",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-prod": "react-native run-android --variant=release",
    "android-build": "cd android && ./gradlew assembleRelease",
    "android-clear": "cd android && ./gradlew clean",
    "android-studio": "open -a /Applications/Android\\ Studio.app",
    "android-release-folder": "open android/app/build/outputs/apk/release",
    "ios": "react-native run-ios --simulator='iPhone 14'",
    "pod": "cd ios && pod install && cd ..",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "axios": "^1.1.3",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-camera": "^4.2.1",
    "react-native-paper": "^5.0.0-rc.10",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
